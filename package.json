{
  "name": "angular-runtime-dependency-mapper",
  "version": "0.0.1",
  "description": "generates dependency map of your angular 1x app at runtime",
  "main": "index.js",
  "scripts": {
    "test:dev": "./node_modules/.bin/jest --watch",
    "test": "./node_modules/.bin/jest --coverage",
    "build": "./node_modules/.bin/babel -d ./dist ./src -s"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniellizik/angular-runtime-dependency-mapper.git"
  },
  "keywords": [
    "angular",
    "dependencies"
  ],
  "author": "daniel lizik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daniellizik/angular-runtime-dependency-mapper/issues"
  },
  "homepage": "https://github.com/daniellizik/angular-runtime-dependency-mapper#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-root-import": "^5.0.0",
    "babel-plugin-syntax-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.22.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "jsdom": "^9.11.0"
  },
  "dependencies": {
    "escodegen": "^1.8.1",
    "esprima": "^3.1.3",
    "parse5": "^3.0.2"
  }
}
